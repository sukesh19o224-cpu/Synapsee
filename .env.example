# Environment Variables for Synapse
# Copy this file to .env and fill in the values

# ===========================================
# GENERAL
# ===========================================
DOMAIN=localhost
PROTOCOL=http

# ===========================================
# APPWRITE
# ===========================================
APPWRITE_ENDPOINT=http://localhost/v1
APPWRITE_PROJECT_ID=synapse
APPWRITE_API_KEY=your-api-key-here
APPWRITE_DATABASE_ID=synapse-db

# ===========================================
# ELABFTW
# ===========================================
ELABFTW_SECRET_KEY=change-this-to-a-random-string
ELABFTW_DB_HOST=elabftw-mysql
ELABFTW_DB_NAME=elabftw
ELABFTW_DB_USER=elabftw
ELABFTW_DB_PASSWORD=change-this-password

# ===========================================
# ONLYOFFICE
# ===========================================
ONLYOFFICE_JWT_SECRET=change-this-jwt-secret

# ===========================================
# WEKNORA
# ===========================================
WEKNORA_LLM_API_KEY=your-llm-api-key
WEKNORA_EMBEDDING_MODEL=text-embedding-3-small

# ===========================================
# MINIO (S3 Storage)
# ===========================================
MINIO_ROOT_USER=synapse
MINIO_ROOT_PASSWORD=change-this-password

# ===========================================
# TRAEFIK
# ===========================================
TRAEFIK_ACME_EMAIL=your-email@example.com
